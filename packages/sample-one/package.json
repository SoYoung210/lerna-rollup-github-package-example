  "bugs": {
    "url": "https://github.com/SoYoung210/lerna-rollup-github-package-example/issues"
  },
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/SoYoung210/lerna-rollup-github-package-example.git",
    "directory": "packages/sample-one"
  }
